{
  "name": "jskos-proxy",
  "version": "0.1.0",
  "description": "HTTP proxy to serve JSKOS objects",
  "type": "module",
  "exports": "./server.js",
  "scripts": {
    "dev": "nodemon --watch .env --watch lib/ --watch '*.js' server.js",
    "start": "npm run dev",
    "test": "NODE_ENV=test mocha --exit",
    "lint": "eslint --ignore-path .gitignore --ext .js,.vue .",
    "fix": "npm run lint -- --fix",
    "build": "vite build",
    "serve": "NODE_ENV=production node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gbv/jskos-proxy.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gbv/jskos-proxy/issues"
  },
  "homepage": "https://github.com/gbv/jskos-proxy#readme",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "cocoda-sdk": "^3.4.5",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "jskos-tools": "^1.0.31",
    "jskos-vue": "^0.2.7",
    "jsonld": "^8.1.1",
    "nodemon": "^2.0.22",
    "portfinder": "^1.0.32",
    "rdflib": "^2.2.32",
    "serve-static": "^1.15.0",
    "vite": "^4.3.7",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "eslint": "^8.40.0",
    "eslint-config-gbv": "^1.0.5",
    "eslint-plugin-vue": "^9.13.0",
    "mocha": "^10.2.0",
    "mocha-eslint": "^7.0.0"
  }
}
